CREATE TABLE amarouno_masteraip ( id uuid, device_id varchar, device_type varchar, system_date timestamp,sample_id float, location varchar, outside_temperature float, device_date timestamp, id_tenant int, id_enterprise int,  PRIMARY KEY ((device_id, device_type), system_date)) WITH CLUSTERING ORDER BY (system_date DESC); CREATE INDEX IF NOT EXISTS amarouno_masteraip_id ON amarouno_masteraip(id);CREATE INDEX IF NOT EXISTS amarouno_masteraip_id_enterprise ON amarouno_masteraip(id_enterprise); CREATE INDEX IF NOT EXISTS amarouno_masteraip_id_tenant ON amarouno_masteraip(id_tenant);
